{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Content-Security-Policy": "frame-ancestors 'self' https://www.notion.so https://www.notion.site",
        "X-Frame-Options": "ALLOWALL"
      },
      "dest": "/$1"
    }
  ]
}
